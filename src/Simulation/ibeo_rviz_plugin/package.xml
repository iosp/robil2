  <description>
     a plugin for the ibeo integration in rviz
  </description>
  <maintainer email="yderi@iai.co.il">Yuval Deri</maintainer>
  <license>BSD</license>

  <url>http://</url>
  <author>Yuval</author>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>rviz</build_depend>
  <build_depend>gazebo_ibeo_gpu</build_depend>
<build_depend>message_runtime</build_depend>
