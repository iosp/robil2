<?xml version="1.0"?>
<package>
  <name>bobtank_plugin</name>
  <version>0.1.0</version>
  <description>The bobtank robot by or</description>

  <maintainer email="ortslil64@gmail.com">Or Tslil</maintainer>


  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>   <!-- Compilation Library -->
